<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.manyi.iw.contract.soa.server.mybatis.dao.contract.write.custom.AgentContractBillWriteDao" >
    <select id="selectByContractIdLock" resultType="com.manyi.iw.contract.soa.api.model.po.mbg.AgentContractBill">
        select
        <include refid="com.manyi.iw.contract.soa.server.mybatis.dao.contract.write.mbg.MBGAgentContractBillWriteDao.Base_Column_List"/>
        from iw_agent_contract_bill
        where contractId = #{contractId,jdbcType=BIGINT} and state != #{state} AND type = #{billType}
        <if test="lock != null and lock != ''">
            ${lock}
        </if>
    </select>
</mapper>