<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.manyi.iw.contract.soa.server.mybatis.dao.read.custom.SaleContractCityApproveConfigReadDao">

    <select id="getCityApproveConfigByCityId" lang="velocity" parameterType="java.lang.Long"
            resultType="com.manyi.iw.contract.soa.api.model.po.mbg.SaleContractCityApproveConfig">
      SELECT
      <include refid="com.manyi.iw.contract.soa.server.mybatis.dao.read.mbg.MBGSaleContractCityApproveConfigReadDao.Base_Column_List"/>
      from iw_sale_contract_city_approve_config
      where cityId = @{cityId} limit 1
    </select>
</mapper>

