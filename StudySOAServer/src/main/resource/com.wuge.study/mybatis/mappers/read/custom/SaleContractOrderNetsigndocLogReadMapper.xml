<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.manyi.iw.contract.soa.server.mybatis.dao.read.custom.SaleContractOrderNetsigndocLogReadDao">

    <select id="countNetSignDocLog" parameterType="com.manyi.iw.contract.soa.api.model.vo.sale.ListNetSignDocLogParam"
            resultType="int">
    select
    count(r.id)
    from iw_sale_contract_order_netsigndoc_log r
    where r.orderId = #{param.orderId}
    </select>
</mapper>