<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.manyi.iw.contract.soa.server.mybatis.dao.read.custom.SaleContractOrderBeCommentAgentReadDao">

	<select id="queryOverTime" resultType="com.manyi.iw.contract.soa.api.model.po.mbg.SaleContractOrderBeCommentAgent">
		SELECT
		<include refid="com.manyi.iw.contract.soa.server.mybatis.dao.read.mbg.MBGSaleContractOrderBeCommentAgentReadDao.Base_Column_List"/>
		FROM iw_sale_contract_order_be_comment_agent WHERE now() >= DATE_ADD(createTime,INTERVAL 5 DAY) AND commentTime IS NULL
	</select>


	<select id="selectBeCommentNamesByCommentId" resultType="java.lang.String" parameterType="java.lang.Long">
		SELECT bc.beCommentAgentName FROM  iw_sale_contract_order_be_comment_agent bc WHERE bc.commentId = #{commentId}
	</select>

</mapper>